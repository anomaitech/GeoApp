<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2"
     xmlns:gx="http://www.google.com/kml/ext/2.2"
     xmlns:kml="http://www.opengis.net/kml/2.2"
     xmlns:atom="http://www.w3.org/2005/Atom">
<Document>
    <name>Map with Flags</name>

    <Style id="highlightStyle">
        <IconStyle>
            <scale>1.2</scale>
            <Icon>
                <href>https://earth.google.com/earth/document/icon?color=1976d2&amp;id=2000&amp;scale=4</href>
            </Icon>
        </IconStyle>

    </Style>

    <!-- Placemark: Addis Ababa (Ethiopia) -->
    <Placemark id="addisababa">
        <name>Addis Ababa</name>
        <description><![CDATA[




            <p><strong>Capital of Ethiopia</strong></p>

            <p>Addis Ababa is the capital city of Ethiopia, often referred to as the political capital of Africa due to its diplomatic significance. It is the headquarters of the African Union.</p>
            <p>
                <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/7/71/Flag_of_Ethiopia.svg/800px-Flag_of_Ethiopia.svg.png"
                     alt="Flag of Ethiopia"
                     style="width:50px;" />
            </p>
        ]]></description>
        <Point>
            <!-- KML uses (longitude, latitude, altitude) -->
            <coordinates>38.7578,8.9806,0</coordinates>
        </Point>
    </Placemark>

    <!-- Placemark: Jerusalem (Israel) -->
    <Placemark id="israel">
        <name>Jerusalem</name>
        <description><![CDATA[
            <p><strong>City in Israel</strong></p>
            <p>Jerusalem is one of the oldest cities in the world, held sacred by Judaism, Christianity, and Islam. It is a focal point of religious and historical significance.</p>
            <p>
                <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/d/d4/Flag_of_Israel.svg/800px-Flag_of_Israel.svg.png"
                     alt="Flag of Israel"
                     style="width:50px;" />
            </p>
        ]]></description>
        <Point>
            <coordinates>35.220480,31.778345,0</coordinates>
        </Point>
    </Placemark>

    <!-- Placemark: Delft (Netherlands) -->
    <Placemark id="netherlands">
        <name>Delft</name>
        <description><![CDATA[
            <p><strong>City in the Netherlands</strong></p>
            <p>Delft is renowned for its historic canals, Delft Blue pottery, and its ties to the Dutch royal family. It is a vibrant university city near The Hague.</p>
            <p>
                <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/2/20/Flag_of_the_Netherlands.svg/800px-Flag_of_the_Netherlands.svg.png"
                     alt="Flag of the Netherlands"
                     style="width:50px;" />
            </p>
        ]]></description>
        <Point>
            <coordinates>4.357067,52.011576,0</coordinates>
        </Point>
    </Placemark>

    <!-- Placemark: Nashville (USA) -->
    <Placemark id="usa">
        <name>Nashville</name>
        <description><![CDATA[
            <p><strong>Capital of Tennessee, USA</strong></p>
            <p>Nashville is famous for its country music scene, earning it the nickname "Music City." It is also home to numerous universities and cultural institutions.</p>
            <p>
                <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/a/a4/Flag_of_the_United_States.svg/800px-Flag_of_the_United_States.svg.png"
                     alt="Flag of the USA"
                     style="width:50px;" />
            </p>
        ]]></description>
        <Point>
            <coordinates>-86.5804,35.5175,0</coordinates>
        </Point>
    </Placemark>


</Document>
</kml>
